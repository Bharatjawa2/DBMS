CREATE TABLE EMPL(
EMPL_ID INT PRIMARY KEY,
EMPL_NAME VARCHAR(255),
ADDRESS VARCHAR(255),
DESIGNATION VARCHAR(255)
)
INSERT INTO EMPL(EMPL_ID,EMPL_NAME,ADDRESS,DESIGNATION)VALUES(1,'A','NOIDA','MANAGER');
INSERT INTO EMPL(EMPL_ID,EMPL_NAME,ADDRESS,DESIGNATION)VALUES(2,'B','DELHI','TEAM LEADER');
INSERT INTO EMPL(EMPL_ID,EMPL_NAME,ADDRESS,DESIGNATION)VALUES(3,'C','NOIDA','TEAM MEMBER');
INSERT INTO EMPL(EMPL_ID,EMPL_NAME,ADDRESS,DESIGNATION)VALUES(4,'D','AMRITSAR','TEAM LEADER');
INSERT INTO EMPL(EMPL_ID,EMPL_NAME,ADDRESS,DESIGNATION)VALUES(5,'E','DELHI','MANAGER');

SELECT * FROM EMPL;
SELECT * FROM EMPL WHERE DESIGNATION='MANAGER';
SELECT * FROM EMPL WHERE EMPL_ID=5;
SELECT * FROM EMPL WHERE DESIGNATION='MANAGER' AND ADDRESS='NOIDA';
SELECT * FROM EMPL WHERE EMPL_ID>=2 AND EMPL_ID<4;
SELECT * FROM EMPL WHERE EMPL_ID BETWEEN 2 AND 4;
SELECT * FROM EMPL WHERE ADDRESS='DELHI' OR ADDRESS='NOIDA';
SELECT * FROM EMPL WHERE ADDRESS!='DELHI';

ALTER TABLE EMPL ADD DOB DATE;
UPDATE EMPL SET DOB=TO_DATE('18/06/2024','DD/MM/YYYY');
UPDATE EMPL SET DOB=TO_DATE('15/05/2024','DD/MM/YYYY') WHERE EMPL_ID BETWEEN 2 AND 4;

INSERT INTO EMPL(EMPL_ID,EMPL_NAME,ADDRESS,DESIGNATION,DOB)VALUES(6,'F','MUMBAI','TEAM_LEADER',TO_DATE('25/05/2024','DD/MM/YYYY'));
SELECT * FROM EMPL WHERE ADDRESS LIKE '%MUM%';
SELECT * FROM EMPL WHERE EMPL_NAME LIKE 'C%';
SELECT * FROM EMPL WHERE ADDRESS LIKE '%I';
SELECT * FROM EMPL WHERE ADDRESS LIKE '*O%';
SELECT * FROM EMPL WHERE DESIGNATION LIKE '%E*';
SELECT * FROM EMPL WHERE ADDRESS LIKE '__I%';
SELECT * FROM EMPL WHERE ADDRESS LIKE 'N%A';

ALTER TABLE EMPL ADD SALARY NUMBER;
UPDATE EMPL SET SALARY=20000 WHERE EMPL_ID=1;
UPDATE EMPL SET SALARY=15000 WHERE EMPL_ID=2;
UPDATE EMPL SET SALARY=50000 WHERE EMPL_ID=3;
UPDATE EMPL SET SALARY=25000 WHERE EMPL_ID=4;
UPDATE EMPL SET SALARY=40000 WHERE EMPL_ID=5;
UPDATE EMPL SET SALARY=15000 WHERE EMPL_ID=6;

SELECT MIN(SALARY) FROM EMPL;
SELECT * FROM EMPL WHERE SALARY = (SELECT MIN(SALARY) FROM EMPL);
